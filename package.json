{
  "name": "@muxiu1997/define-emitter-composable",
  "version": "0.1.0-rc.1",
  "description": "Creates a Vue composable function for providing or injecting a mitt event emitter.",
  "source": "src/index.ts",
  "module": "dist/define-emitter-composable.mjs",
  "main": "dist/define-emitter-composable.js",
  "jsnext:main": "dist/define-emitter-composable.mjs",
  "umd:main": "dist/define-emitter-composable.umd.js",
  "typings": "index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "module": "./dist/define-emitter-composable.mjs",
    "import": "./dist/define-emitter-composable.mjs",
    "require": "./dist/define-emitter-composable.js",
    "default": "./dist/define-emitter-composable.mjs"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "bundle": "microbundle -f es,cjs,umd"
